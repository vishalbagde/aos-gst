<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<grid model="com.axelor.apps.gst.db.State" title="State" name="state-grid">
		<field name="name" if="__config__.app.isApp('gst')" />
		<field name="country" if="__config__.app.isApp('gst')" />
	</grid>

	<form model="com.axelor.apps.gst.db.State" title="State" name="state-form"
		width="large">
		<panel itemSpan="6" title="State Details" name="overviewPanel">
			<field name="name" if="__config__.app.isApp('gst')"/>
			<field name="country" if="__config__.app.isApp('gst')" />
		</panel>
	</form>

</object-views>
